# NourishMind Environment Variables
# Copy this to .env.local and fill in your values

# Supabase (client-side)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Supabase (server-side only — Edge Functions)
SUPABASE_SERVICE_ROLE_KEY=

# Google Gemini AI (server-side — Edge Functions)
GEMINI_API_KEY=

# USDA FoodData Central API (server-side — Edge Functions)
USDA_API_KEY=

# Resend Email (server-side — Edge Functions)
RESEND_API_KEY=

# Meta WhatsApp Cloud API
WHATSAPP_ACCESS_TOKEN=
WHATSAPP_PHONE_NUMBER_ID=
WHATSAPP_VERIFY_TOKEN=

# App Config
VITE_APP_URL=http://localhost:8080
VITE_APP_NAME=NourishMind
